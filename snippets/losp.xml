<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="losp"
           fetch="https://github.com/LESSAOSP"
           revision="refs/heads/14" />

  <remote  name="losp-gitlab"
           fetch="https://gitlab.com/LESSAOSP"
           revision="refs/heads/14" />

   <!-- Build -->
  <project path="build/make" name="build" groups="pdk" remote="losp">
    <linkfile dest="build/CleanSpec.mk" src="CleanSpec.mk" />
    <linkfile dest="build/buildspec.mk.default" src="buildspec.mk.default" />
    <linkfile dest="build/core" src="core" />
    <linkfile dest="build/envsetup.sh" src="envsetup.sh" />
    <linkfile dest="build/target" src="target" />
    <linkfile dest="build/tools" src="tools" />
  </project>
  <project path="build/soong" name="build_soong" groups="pdk,tradefed" remote="losp">
    <linkfile dest="Android.bp" src="root.bp" />
    <linkfile dest="bootstrap.bash" src="bootstrap.bash" />
  </project>

  <!-- Vendor -->
  <project path="vendor/aosp" name="vendor_aosp" remote="losp" />

</manifest>